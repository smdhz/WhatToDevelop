<Window x:Class="WhatToDevelop.View.WinWikiText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="请求数据" Height="300" Width="300" WindowStyle="ToolWindow"
        Loaded="Window_Loaded">
    <DockPanel Background="{DynamicResource BackgroundBrushKey}">
        <Button Name="btnGo" Content="获取数据" Height="24" Margin="10,0,10,10" DockPanel.Dock="Bottom" Click="btnGo_Click"/>
        <TextBox Name="txtPage" Margin="10" AcceptsReturn="True" TextWrapping="Wrap"
                 Background="{DynamicResource BackgroundBrushKey}" Foreground="{DynamicResource ForegroundBrushKey}"
                 GotFocus="txtPage_GotFocus"/>
    </DockPanel>
</Window>
